#!/usr/bin/env bash
# This script displays the content of the current directory in a list format,
# showing only the part of the name after the first dash.

for file in *; do
    # Check if the file is not hidden and has a dash in its name
    if [[ ! $file =~ ^\.[^.].*$ && $file =~ ^[^-]*-(.*)$ ]]; then
        echo "${BASH_REMATCH[1]}"
    fi
done
